<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.daxiang.taojin">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:replace="android:icon, android:theme">
        <activity
            android:name=".ui.MainActivity"
            android:theme="@style/Theme.DrawerLayout">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.PicturesActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ui.HttpTestActivity"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".ui.HttpClientRequestActivity"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".ui.OkHttpRequestActivity"
            android:screenOrientation="portrait"></activity>
    </application>

</manifest>